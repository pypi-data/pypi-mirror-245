[windows]
{% for host in vars.groups.windows %}
{{ host }}  distronode_host={{ hostvars[host]['distronode_host'] }} distronode_user={{ hostvars[host]['distronode_user'] }}{{ ' distronode_ssh_private_key_file=' ~ hostvars[host]['distronode_ssh_private_key_file'] if (hostvars[host]['distronode_ssh_private_key_file']|default()) else '' }}
{% endfor %}

[windows:vars]
distronode_shell_type={{ test_shell_type }}
distronode_connection=ssh
distronode_port=22
# used to preserve the existing environment and not touch existing files
distronode_ssh_extra_args="-o UserKnownHostsFile=/dev/null"
distronode_ssh_host_key_checking=False
