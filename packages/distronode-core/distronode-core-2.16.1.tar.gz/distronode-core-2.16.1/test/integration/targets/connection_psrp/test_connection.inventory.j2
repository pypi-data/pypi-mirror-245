[windows]
{% for host in vars.groups.windows %}
{{ host }}  distronode_host={{ hostvars[host]['distronode_host'] }} distronode_port={{ hostvars[host]['distronode_port'] }} distronode_user={{ hostvars[host]['distronode_user'] }} distronode_password={{ hostvars[host]['distronode_password'] }}
{% endfor %}

[windows:vars]
distronode_connection=psrp
distronode_psrp_auth=negotiate
distronode_psrp_cert_validation=ignore
