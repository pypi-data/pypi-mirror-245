{
    "control": {
        "overwrite": true,
        "cluster": false,
        "local": false
    },
    "folder": {
        "folder_output": "slurm_output",
        "folder_delete": ["data_output"],
        "folder_create": ["data_output"]
    },
    "pragmas": {
        "time":  "4:00:00",
        "nodes":  "1",
        "ntasks-per-node": "2",
        "mem":  "8G"
    },
    "vars": {"PYTHONUNBUFFERED": "1"},
    "pre_proc_commands": [
        {
            "tag": "version",
            "executable": "python3",
            "arguments": ["--version"]
        }
    ],
    "post_proc_commands": []
}
