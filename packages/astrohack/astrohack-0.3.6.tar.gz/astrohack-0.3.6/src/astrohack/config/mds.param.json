{
    "plot_antennas":{
        "destination":{
            "type":["string"],
            "nullable": false
        },
        "ant":{
            "type":["ndarray", "int", "list", "string"],
            "struct type": ["int", "string"],
            "nullable": false
        },
        "ddi":{
            "type":["ndarray", "int", "list", "string"],
            "struct type": ["int", "string"],
            "nullable": false
        },
        "plot_type":{
            "type":["string"],
            "allowed":["deviation", "phase", "ancillary", "all"],
            "nullable": false
        },
        "plot_screws":{
            "type":["boolean"],
            "nullable": false
        },
        "amplitude_limits":{
            "type":["ndarray", "list", "tuple"],
            "struct type": ["int", "float"],
            "nullable": true
        },
        "phase_unit":{
            "type":["string"],
            "check allowed with": "units.trig",
            "nullable": false
        },
        "phase_limits":{
            "type":["ndarray", "list", "tuple"],
            "struct type": ["int", "float"],
            "nullable": true
        },
        "deviation_unit":{
            "type":["string"],
            "check allowed with": "units.length",
            "nullable": false
        },
        "deviation_limits":{
            "type":["ndarray", "list", "tuple"],
            "struct type": ["int", "float"],
            "nullable": true
        },
        "panel_labels":{
            "type":["boolean"],
            "nullable": false
        },
        "display":{
            "type":["boolean"],
            "nullable": false
        },
        "colormap":{
            "type":["string"],
            "nullable": false
        },
        "figure_size":{
            "type":["ndarray", "list", "tuple"],
            "struct type": ["int", "float"]
        },
        "dpi":{
            "type":["int"],
            "nullable": false
        },
        "parallel":{
            "type":["boolean"],
            "nullable": false
        },
        "overwrite":{
            "type":["boolean"],
            "nullable": false
        }
    }
}
